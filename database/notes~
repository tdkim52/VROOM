Query:

CREATE TABLE `tdk_vsp`.`hazards` ( `id` INT NOT NULL AUTO_INCREMENT , `type` VARCHAR(20) NULL , `state` VARCHAR(20) NULL , `coordinates` VARCHAR(23) NULL , `message` VARCHAR(50) NULL , PRIMARY KEY (`id`) ) ENGINE = MyISAM;
